page{background-color:#f4f4f4}.container{height:370rpx;background:linear-gradient(90deg, #00b0d6, #00d6d3);display:flex;justify-content:space-around;color:#98fefa;font-size:28rpx}.container .title{margin-top:66rpx}.main{padding:0 25rpx;position:relative}.main .box{height:400rpx;background-color:#fff;margin-top:-169rpx}.main .box .top{height:110rpx;border-bottom:1rpx solid #ddd;padding:20rpx 32rpx;box-sizing:border-box}.main .box .top .market{font-weight:900}.main .box .top .date{font-size:22rpx;color:#666;margin-top:13rpx}.main .box .bottom{height:289rpx;overflow:hidden;padding-top:57rpx;box-sizing:border-box}.main .box .bottom .section{display:flex;flex-direction:column;align-items:center}.main .box .bottom .section .exponent{color:#f02222;font-size:46rpx}.main .box .bottom .section .subtitle{color:#999;font-size:22rpx}.main .box .bottom .numList{margin-top:30rpx;display:flex;justify-content:space-around;position:relative}.main .box .bottom .numList .list{display:flex;flex-direction:column;align-items:center}.main .box .bottom .numList .list .num{font-size:38rpx}.main .box .bottom .numList .list .today{font-size:22rpx;color:#999}.main .box .bottom .numList .line1,.main .box .bottom .numList .line2{position:absolute;background-color:#ddd;width:1rpx;height:40rpx;top:15rpx}.main .box .bottom .numList .line1{left:240rpx}.main .box .bottom .numList .line2{right:232rpx}.main .circle1,.main .circle2{width:25rpx;height:25rpx;background:linear-gradient(90deg, #00b0d6, #00d6d3);border-radius:50%;position:absolute;top:95rpx}.main .circle1{left:13rpx}.main .circle2{right:13rpx}