.homes{height:210rpx;padding:35rpx 0;box-sizing:border-box;display:flex;border-bottom:1rpx solid #ddd}.homes .left image{width:200rpx;height:140rpx}.homes .right{display:flex;flex-direction:column;justify-content:space-around;margin-left:20rpx}.homes .right .title{font-size:30rpx;color:#333}.homes .right .title text{border-left:1rpx solid #ccc;padding-left:10rpx}.homes .right .detail{display:flex}.homes .right .detail .road{font-size:22rpx;color:#999;margin-right:62rpx}.homes .right .detail .sum{color:#ee3e3e;font-weight:900}.homes .right .location{display:flex}.homes .right .location .loc{width:110rpx;height:36rpx;text-align:center;background-color:#eee;line-height:36rpx;font-size:20rpx;margin-right:10rpx}