page {
  height: 100%;
  font-family: PingFang-SC-Medium;
  color: #111;
  font-size: 26rpx;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
swiper {
  height: 455rpx;
  navigator {
    height: 100%;
    image {
      height: 100%;
      width: 100%;
