.tab-bar{display:flex;height:108rpx;background-color:#00b4d7}.tab-bar .item{flex:1;text-align:center;line-height:100rpx;font-size:26rpx;color:#fff}.tab-bar .item.active{color:#fff;font-weight:900;position:relative}.tab-bar .item.active::after{content:'';position:absolute;width:24rpx;height:6rpx;background-color:#fff;bottom:0;left:50%;transform:translateX(-50%)}