.container {
  height: 176rpx;
  background-color: #00b4d7;
  overflow: hidden;
  position: relative;
  z-index: 222;
  .header {
    display: flex;
    margin-top: 20rpx;
    padding: 0 83rpx;
    image {
      width: 24rpx;
      height: 36rpx;
      margin-left: 30rpx;
      margin-top: 10rpx;
    }
  }
  .lists {
    display: flex;
    color: #fff;

    justify-content: space-around;
    align-items: center;
    margin-top: 48rpx;
    text {
      font-size: 20rpx;
    }
  }
}
.main {
  padding: 30rpx 24rpx;
  background-color: #f4f4f4;
  height: 100%;
  overflow: hidden;
  .top {
    display: flex;
    align-items: center;
    justify-content: space-around;
    height: 100rpx;
    background-color: #fff;
    overflow: hidden;
    view {
      position: relative;
      font-size: 22rpx;
      width: 140rpx;
      height: 48rpx;
      background-color: #d8f8ff;
      color: #00b2d6;
      line-height: 48rpx;
      text-align: center;
      text {
        position: absolute;
        right: -5rpx;
        bottom: -15rpx;
        font-size: 22rpx;
      }
    }
  }
  .bottom {
    display: flex;
    border-bottom: 1rpx solid #ddd;
    padding: 30rpx 0;
    .left {
      width: 50%;
      image {
        width: 340rpx;
        height: 230rpx;
      }
    }
    .right {
      width: 50%;
      height: 230rpx;
      display: flex;
      flex-direction: column;
      justify-content: space-around;
      .title {
        font-size: 30rpx;
        color: #333;
        text {
        }
      }
      .detail {
        color: #999;
        font-size: 22rpx;
      }
      .particulars {
        view {
          float: left;

          font-size: 20rpx;
          width: 110rpx;
          height: 36rpx;
          background-color: #eee;
          line-height: 36rpx;
          text-align: center;
          margin-right: 6rpx;
          margin-bottom: 5rpx;
        }
      }
      .money {
        display: flex;
        justify-content: flex-end;

        color: #ee3e3e;
        font-weight: bold;
      }
    }
  }
}
// 遮罩
.mask {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 88;
  background: rgba(102, 102, 102, 1);
}
// 区域
.modalFrame {
  height: 550rpx;
  width: 100%;
  position: absolute;
  left: 0;
  top: 175rpx;
  z-index: 999;

  .top {
    height: 460rpx;
    display: flex;
    .left {
      width: 50%;
      background-color: #eeeeee;

      color: #666;
      view {
        height: 80rpx;
        width: 100%;
        line-height: 80rpx;
        text-align: center;
      }
    }
    .right {
      width: 50%;
      background-color: #fff;

      color: #666;
      view {
        height: 80rpx;
        width: 100%;
        line-height: 80rpx;
        text-align: center;
      }
    }
  }
  .bottom {
    height: 90rpx;
    background-color: #fff;
    display: flex;
    justify-content: space-around;
    align-items: center;
    font-size: 22rpx;
    .clear,
    .confirm {
      width: 200rpx;
      height: 48rpx;
      background: rgba(238, 238, 238, 1);
      border-radius: 4rpx;
      text-align: center;
      line-height: 48rpx;
      color: #999;
    }
    .confirm {
      background-color: #00b2d6;
      color: #fff;
    }
  }
}
// 面积
.modelArea {
  height: 450rpx;
  position: absolute;
  left: 0;
  top: 175rpx;
  z-index: 999;
  width: 100%;
  .top {
    height: 310rpx;
    padding: 0 34rpx;
    box-sizing: border-box;
    background-color: #eee;
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    justify-content: space-between;
    view {
      width: 200rpx;
      height: 56rpx;
      background: #fff;
      border-radius: 4rpx;
      font-size: 24rpx;
      color: #666;
      line-height: 56rpx;
      text-align: center;
    }
  }
  .bottom {
    height: 140rpx;
    overflow: hidden;
    position: relative;
    background-color: #fff;
    .totalArea {
      font-size: 22rpx;

      font-weight: bold;
      margin-left: 44rpx;
      margin-top: 20rpx;
      height: 22rpx;
    }
    .totalArea::before {
      content: '';
      position: absolute;
      top: 22rpx;
      left: 24rpx;
      height: 24rpx;
      width: 8rpx;
      background-color: #00b2d6;
    }
    .count {
      display: flex;
      margin-top: 40rpx;
      height: 30rpx;
      .left {
        height: 44rpx;

        color: #999;
        display: flex;
        justify-content: space-around;
        align-items: center;
        input {
          width: 120rpx;
          border-bottom: 1rpx solid #ddd;
          text-align: center;
        }
        .down {
          margin-left: 25rpx;
        }
      }
      .to {
        font-weight: bold;
        padding: 0 30rpx;
      }
    }
    .ensure {
      width: 120rpx;
      height: 44rpx;
      background: rgba(0, 178, 214, 1);
      border-radius: 4rpx;
      text-align: center;
      line-height: 44rpx;
      position: absolute;
      color: #fff;
      font-size: 24rpx;
      right: 23rpx;
      bottom: 19rpx;
    }
  }
}
// 费用
.modelPrice {
  height: 400rpx;
  position: absolute;
  left: 0;
  top: 175rpx;
  z-index: 999;
  width: 100%;
  display: flex;
  .left {
    width: 260rpx;
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;

    font-weight: bold;
    color: #666;
    view {
      padding: 35rpx 0;
    }
  }
  .right {
    flex: 1;
    background-color: #eee;
    .head {
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: space-around;

      color: #666;
      height: 288rpx;
    }
    .foot {
      height: 112rpx;
      position: relative;
      .customPrice {
        font-size: 22rpx;

        margin-left: 54rpx;
        font-weight: 900;
      }
      .customPrice::before {
        content: '';
        position: absolute;
        top: 5rpx;
        left: 30rpx;
        height: 24rpx;
        width: 8rpx;
        background-color: #00b2d6;
      }
      .prices {
        display: flex;
        align-items: center;
        padding: 20rpx 30rpx;
        input {
          width: 90rpx;
          border-bottom: 1rpx solid #ddd;
          text-align: center;
        }
        .to {
          font-weight: bold;
          padding: 0 30rpx;
        }
        .botton {
          width: 100rpx;
          height: 44rpx;
          background: rgba(0, 178, 214, 1);
          border-radius: 4rpx;
          color: #fff;
          font-size: 24rpx;
          text-align: center;
          line-height: 44rpx;
          margin-left: 139rpx;
        }
      }
    }
  }
}
