@import "./../components/tabBars.wxss";
page{background-color:#f4f4f4}.tab-content .total{padding:20rpx 24rpx}.tab-content .total .serve{height:280rpx;margin-bottom:20rpx;background:linear-gradient(90deg, #00b0d6, #00d6d3)}.tab-content .total .serve .header{display:flex;justify-content:space-between;height:64rpx;align-items:center;padding:0 19rpx;border-bottom:1rpx solid rgba(255,255,255,0.3)}.tab-content .total .serve .header .left{font-size:24rpx;color:#fff}.tab-content .total .serve .header .right{width:100rpx;height:40rpx;background-color:#e3faff;color:#00b4d7;font-weight:900;font-size:22rpx;line-height:40rpx;text-align:center}.tab-content .total .serve .footer{height:215rpx;display:flex;padding:45rpx 0;box-sizing:border-box}.tab-content .total .serve .footer .image{width:130rpx;display:flex;justify-content:space-around;align-items:center}.tab-content .total .serve .footer .image image{width:58rpx;height:58rpx}.tab-content .total .serve .footer .detail{font-size:24rpx;color:#fff;display:flex;flex-direction:column;justify-content:space-around;flex:1}.tab-content .total .serve .footer .detail text{margin-left:30rpx}